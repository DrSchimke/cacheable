{
    "name": "sci/cacheable",
    "description": "Transparent caching library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "psr/cache": "^1.0",
        "psr/cache-implementation": "^1.0"
    },
    "require-dev": {
        "ext-redis": "*",
        "phpunit/phpunit": "^4.7",
        "cache/doctrine-adapter": "dev-master",
        "cache/redis-adapter": "dev-master",
        "cache/chain-adapter": "^0.2.0",
        "cache/array-adapter": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Sci\\Cacheable\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sci\\Tests\\Cacheable\\": "tests/"
        }
    }
}
